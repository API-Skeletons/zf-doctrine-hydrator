{
    "name": "api-skeletons/zf-doctrine-hydrator",
    "description": "A collection of common hydrators for phpro/zf-doctrine-hydration-module",
    "type": "library",
    "require": {
        "php": "^7.1",
        "phpro/zf-doctrine-hydration-module": "^3.0",
        "zendframework/zend-eventmanager": "^2.4 || ~3.0",
        "zfcampus/zf-apigility-doctrine": "^2.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-modulemanager": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "zfcampus/zf-apigility": "*",
        "zfcampus/zf-configuration": "*",
        "doctrine/doctrine-orm-module": "*",
        "doctrine/data-fixtures": "*",
        "zendframework/zend-test": "*",
        "zendframework/zend-serializer": "*",
        "zendframework/zend-log": "*",
        "zendframework/zend-config": "*",
        "zendframework/zend-console": "*",
        "zendframework/zend-i18n": "*",
        "api-skeletons/coding-standard": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tom H Anderson",
            "email": "tom.h.anderson@gmail.com"
        },
        {
            "name": "Marcelo Insabralde",
            "email": "marcelominsabralde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ZF\\Doctrine\\Hydrator\\": "src/",
            "ZFTest\\Doctrine\\Hydrator\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "extra": {
        "zf": {
            "module": "ZF\\Doctrine\\Hydrator"
        }
    }
}
